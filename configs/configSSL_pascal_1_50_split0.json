{
  "model": "DeepLab",
  "version": "2",
  "dataset": "pascal_voc",

  "training": {
    "batch_size":  14,
    "num_workers": 3,
    "optimizer": "SGD",
    "momentum": 0.9,
    "num_iterations": 80000,
    "learning_rate": 2e-4,
    "lr_schedule": "Poly",
    "lr_schedule_power": 0.9,
    "weight_decay": 5e-4,
    "use_sync_batchnorm": false,

    "data": {
      "split_id_list": 0,
      "labeled_samples": 212,
      "input_size": "321,321"
    },
    "unlabeled": {
      "train_unlabeled": true,
      "consistency_weight": 1,
      "consistency_loss": "CE",
      "pixel_weight": "sigmoid"
    }

