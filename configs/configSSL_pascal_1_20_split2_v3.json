{
  "model": "DeepLab",
  "version": "3",
  "dataset": "pascal_voc",

  "training": {
    "batch_size":  20,
    "num_workers": 3,
    "optimizer": "SGD",
    "momentum": 0.9,
    "num_iterations": 80000,
    "learning_rate": 2e-4,
    "lr_schedule": "Poly",
    "lr_schedule_power": 0.9,
    "weight_decay": 5e-4,
    "pretraining": "imagenet",
    "data": {
      "split_id_list": 2,
      "labeled_samples": 530,
      "input_size": "321,321"
    }
  },
  "seed": 7,
  "ignore_label": 255,

  "utils": {
    "save_checkpoint_every": 10000,
    "checkpoint_dir": "../saved/DeepLab",
    "val_per_iter": 1000,
    "save_best_model": true
  }
}

